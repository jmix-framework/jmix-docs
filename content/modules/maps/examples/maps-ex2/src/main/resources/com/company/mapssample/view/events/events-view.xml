<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<view xmlns="http://jmix.io/schema/flowui/view" xmlns:maps="http://jmix.io/schema/maps/ui"
      title="msg://eventsView.title">
    <layout>
        <maps:geoMap id="geoMap" width="100%"
                     height="100%">
            <maps:mapView zoom="4"
                          centerX="20"
                          centerY="45"/>
            <maps:layers>
                <maps:tile>
                    <maps:osmSource/>
                </maps:tile>
                <maps:vector id="vector">
                    <maps:vectorSource/>
                </maps:vector>
            </maps:layers>
        </maps:geoMap>
    </layout>
</view>